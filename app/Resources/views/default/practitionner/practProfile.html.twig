{% extends "base.html.twig" %}
{% block body %}
    <div class="container-white container-pract-profile">
        <div class="banner">
            <img src="{{ asset('img/profile/banners/banner_pract_1.jpg') }}" alt="">
        </div>
        <div class="pract-type">
            Type du practitien
        </div>
        <p>{{ user.name }} {{ user.firstname }}</p>
        <p>SIRET : {{ user.practInfos.siret }}</p>
        <div id="map"></div>
        <script>

            function initMap() {
                var mapDiv = document.getElementById('map');
                var map = new google.maps.Map(mapDiv, {
                    center: {lat: 44.540, lng: -78.546},
                    zoom: 8
                });
            }
        </script>

        <script async defer
                src="https://maps.googleapis.com/maps/api/js?key=YOUR_API_KEY&callback=initMap">
        </script>
    </div>
{% endblock %}