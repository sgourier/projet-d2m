{% extends 'SonataAdminBundle::standard_layout.html.twig' %}
{% block sonata_admin_content %}
    {% for video in videos %}
        <h2>{{ video.title }}</h2>
        <iframe width="420" height="315" src="http://www.youtube.com/embed/{{ video.id }}" frameborder="0" allowfullscreen value="{{ 'http://www.youtube.com/v/' ~  video.id }}"></iframe>
        <p>{{ video.description }}</p>
    {% endfor %}
{% endblock %}
