<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="VimoliaBundle\Entity\Article" table="article">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="text" type="text" column="text" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateadd" type="datetime" column="dateAdd" nullable="true"/>
    <field name="dateupd" type="datetime" column="dateUpd" nullable="true"/>
    <field name="active" type="boolean" column="active" nullable="true"/>
  </entity>
</doctrine-mapping>
