<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="VimoliaBundle\Entity\Practevals" table="practevals">
    <indexes>
      <index name="fk_PractEvals_User1_idx" columns="id_owner"/>
      <index name="fk_PractEvals_User2_idx" columns="id_pract"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="note" type="float" column="note" precision="10" scale="0" nullable="true"/>
    <field name="text" type="text" column="text" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateadd" type="datetime" column="dateAdd" nullable="true"/>
    <field name="dateupd" type="datetime" column="dateUpd" nullable="true"/>
    <field name="active" type="boolean" column="active" nullable="true"/>
    <field name="dateconsultation" type="datetime" column="dateConsultation" nullable="true"/>
    <field name="practevalscol" type="string" column="PractEvalscol" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idPract" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="id_pract" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idOwner" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="id_owner" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
