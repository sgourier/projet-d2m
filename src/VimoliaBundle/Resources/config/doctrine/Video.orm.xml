<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="VimoliaBundle\Entity\Video" table="video">
    <indexes>
      <index name="fk_Video_VideoCategory1_idx" columns="id_videoCategory"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="1500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="text" column="description" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="url" type="string" column="url" length="1500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateadd" type="datetime" column="dateAdd" nullable="true"/>
    <field name="dateupd" type="datetime" column="dateUpd" nullable="true"/>
    <field name="active" type="boolean" column="active" nullable="true"/>
    <many-to-one field="idVideocategory" target-entity="Videocategory" fetch="LAZY">
      <join-columns>
        <join-column name="id_videoCategory" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
