<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="VimoliaBundle\Entity\Subscribe" table="subscribe">
    <indexes>
      <index name="fk_Subscribe_SubscribeType1_idx" columns="id_subscribeType"/>
      <index name="fk_Subscribe_User1_idx" columns="id_user"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idCommand" type="string" column="id_command" length="1500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="enddate" type="datetime" column="endDate" nullable="true"/>
    <many-to-one field="idUser" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="id_user" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idSubscribetype" target-entity="Subscribetype" fetch="LAZY">
      <join-columns>
        <join-column name="id_subscribeType" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
