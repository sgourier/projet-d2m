<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="VimoliaBundle\Entity\Practinfos" table="practinfos">
    <indexes>
      <index name="fk_PractInfos_PractDomains1_idx" columns="id_practDomains"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="siret" type="string" column="siret" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="proaddress" type="string" column="proAddress" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="proaddressplus" type="string" column="proAddressPlus" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="procity" type="string" column="proCity" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prozipcode" type="integer" column="proZipCode" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="prophone" type="integer" column="proPhone" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="procellphone" type="integer" column="proCellPhone" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="url" type="string" column="url" length="1500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="text" column="description" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="degrees" type="text" column="degrees" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idPractdomains" target-entity="Practdomains" fetch="LAZY">
      <join-columns>
        <join-column name="id_practDomains" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
