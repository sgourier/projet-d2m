<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="VimoliaBundle\Entity\Discussion" table="discussion">
    <indexes>
      <index name="fk_Discussion_AdvancedInfos1_idx" columns="id_advancedInfos"/>
      <index name="fk_Discussion_User1_idx" columns="id_expert"/>
      <index name="fk_Discussion_User2_idx" columns="id_member"/>
      <index name="fk_Discussion_User3_idx" columns="id_pract"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="public" type="boolean" column="public" nullable="true"/>
    <field name="feedback" type="string" column="feedback" length="150" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateadd" type="datetime" column="dateAdd" nullable="true"/>
    <field name="dateupd" type="datetime" column="dateUpd" nullable="true"/>
    <field name="active" type="boolean" column="active" nullable="true"/>
    <field name="closed" type="boolean" column="closed" nullable="true"/>
    <field name="status" type="string" column="status" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idPract" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="id_pract" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idMember" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="id_member" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idExpert" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="id_expert" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAdvancedinfos" target-entity="Advancedinfos" fetch="LAZY">
      <join-columns>
        <join-column name="id_advancedInfos" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
